<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <servlet>
    <servlet-name>member</servlet-name>
    <servlet-class>com.fnw.control.MemberController</servlet-class>
    <init-param>
      <param-name>properties</param-name>
      <param-value>member.properties</param-value>
    </init-param>

</servlet>

<servlet-mapping>
	<servlet-name>member</servlet-name>
	<url-pattern>*.member</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>market</servlet-name>
	<servlet-class>com.fnw.control.MarketController</servlet-class>
	<init-param>
		<param-name>properties</param-name>
		<param-value>market.properties</param-value>
	</init-param>
</servlet>
<servlet-mapping>
	<servlet-name>market</servlet-name>
	<url-pattern>*.market</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>seat</servlet-name>
	<servlet-class>com.fnw.control.SeatController</servlet-class>
	<init-param>
		<param-name>properties</param-name>
		<param-value>seat.properties</param-value>
	</init-param>
</servlet>
<servlet-mapping>
	<servlet-name>seat</servlet-name>
	<url-pattern>*.seat</url-pattern>
</servlet-mapping>

</web-app>